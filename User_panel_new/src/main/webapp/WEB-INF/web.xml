<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<display-name>LogoutServlet</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AddToCartServlet</servlet-name>
		<display-name>AddToCartServlet</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.AddToCartServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Cart</servlet-name>
		<display-name>Cart</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.entity.Cart</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QuantityIncDecServlet</servlet-name>
		<display-name>QuantityIncDecServlet</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.QuantityIncDecServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MyOrder</servlet-name>
		<display-name>MyOrder</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.MyOrder</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CartLenght</servlet-name>
		<display-name>CartLenght</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.CartLenght</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>OrderDetailServlet</servlet-name>
		<display-name>OrderDetailServlet</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.OrderDetailServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ChatServlet</servlet-name>
		<display-name>ChatServlet</display-name>
		<description></description>
		<servlet-class>com.codeo.shop.controller.ChatServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AddToCartServlet</servlet-name>
		<url-pattern>/AddToCartServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Cart</servlet-name>
		<url-pattern>/Cart</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QuantityIncDecServlet</servlet-name>
		<url-pattern>/QuantityIncDecServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MyOrder</servlet-name>
		<url-pattern>/MyOrder</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CartLenght</servlet-name>
		<url-pattern>/CartLenght</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>OrderDetailServlet</servlet-name>
		<url-pattern>/OrderDetailServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ChatServlet</servlet-name>
		<url-pattern>/ChatServlet</url-pattern>
	</servlet-mapping>
	
</web-app>